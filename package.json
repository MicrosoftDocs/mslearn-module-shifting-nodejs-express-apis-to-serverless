{
  "name": "shifting-express-to-functions",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "node:build-ts": "tsc -p server",
    "node:watch-ts": "tsc -w -p server",
    "node:lint": "npm run node:build-ts && eslint \"server/**/*.ts\" --quiet --fix",
    "node:debug": "WWW=./dist/vacationwishlist nodemon -r dotenv/config --inspect=9229 server/dist/index.js",
    "node:start": "WWW=./dist/vacationwishlist node -r dotenv/config server/dist/index.js",
    "full:build": "npm run node:build-ts && npm run build",
    "full:debug": "concurrently \"npm run node:debug\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@ngrx/data": "^9.0.0",
    "@ngrx/effects": "^9.0.0",
    "@ngrx/entity": "^9.0.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.5.4",
    "tslint": "^6.1.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/jasmine": "^3.5.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "bulma": "^0.8.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tslib": "^1.11.1",
    "typescript": "~3.7.5"
  }
}
